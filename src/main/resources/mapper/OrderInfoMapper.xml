<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.logistics.mapper.OrderInfoMapper">

    <insert id="createOrderInfo" parameterType="com.example.logistics.domain.bean.OrderInfo">
        INSERT into order_info(order_no,order_status,goods_total_num,order_pay_amount,remark,create_time)
        VALUES (#{orderNo},#{orderStatus},#{goodsTotalNum},#{orderPayAmount},#{remark},#{createTime})
    </insert>
</mapper>